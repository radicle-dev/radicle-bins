gcloud beta compute \
--project=opensourcecoin instances create seedling-v${VERSION} \
--description="Growing our garden" \
--zone=europe-west3-c \
--machine-type=e2-medium \
--subnet=default \
--network-tier=PREMIUM \
--maintenance-policy=MIGRATE \
--service-account=269502217285-compute@developer.gserviceaccount.com \
--scopes=https://www.googleapis.com/auth/devstorage.read_only,https://www.googleapis.com/auth/logging.write,https://www.googleapis.com/auth/monitoring.write,https://www.googleapis.com/auth/servicecontrol,https://www.googleapis.com/auth/service.management.readonly,https://www.googleapis.com/auth/trace.append \
--tags=http-server,https-server \
--image=ubuntu-minimal-2010-groovy-v20210119 \
--image-project=ubuntu-os-cloud \
--boot-disk-size=16GB \
--boot-disk-type=pd-standard \
--boot-disk-device-name=state \
--create-disk=mode=rw,size=500,type=projects/opensourcecoin/zones/europe-west3-c/diskTypes/pd-ssd,name=state,device-name=state \
--no-shielded-secure-boot \
--shielded-vtpm \
--shielded-integrity-monitoring \
--reservation-affinity=any \
--metadata startup-script-url=gs://builds.radicle.xyz/radicle-bins/v${VERSION}/dist/setup.sh
